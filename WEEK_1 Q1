#include <iostream>
#include <string>
using namespace std;

int main() {
    int t;
    cin >> t;

    while (t--) {
        int n;
        string s;
        cin >> n >> s;

        
        int count = 0;
        for (int i = 0; i < n; i++) {
            if (s[i] == 'U') {
                count++;
            }
        }

        
        if (count % 2 == 1) {
            cout << "YES" << endl; // Alice wins
        } else {
            cout << "NO" << endl; // Bob wins
        }
    }

}
